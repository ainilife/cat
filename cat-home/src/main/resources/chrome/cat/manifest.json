{
    "manifest_version": 2,
    "name": "CAT Extension",
    "version": "0.1",
    "description": "CAT Extension",
	"permissions": ["<all_urls>","webRequest","webRequestBlocking", "tabs"],
    "background": {
        "scripts": ["cat.js"],
        "persistent": true
    },
    "browser_action": {
    	"default_title": "CAT Logview",
    	"default_icon": "cat.png",
    	"default_popup": "popup.html"
  }
}
